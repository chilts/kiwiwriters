<%init>
    my ($name) = $m->dhandler_arg() =~ m{ \A (\w+) \. html \z }xms;

    $r->content_type('text/html');
    # return if $m->cache_self( key => $name, expire_in => '10s' );

    $m->comp('/zaapt/model/faq/view.mhtml', zaapt => $ZAAPT, f_name => $name);
    return;
</%init>
